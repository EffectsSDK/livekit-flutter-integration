+    fun getZoomLevel(): Double {
+        return (cameraPipeline?.getZoomLevel()!! / 100).toDouble()
+    }
+
+    fun setZoomLevel(zoomLevel: Double) {
+        val intValue = (zoomLevel * 100).toInt()

+-(instancetype)initWithSDKPipeline:(id<TSVBPipeline>)pipeline frameFactory:(id<TSVBFrameFactory> _Nonnull)frameFactory {
+	self = [super init];
+	if (self) {
+		_sdkPipeline = pipeline;
+		_frameFactory = frameFactory;
+		_lock = OS_UNFAIR_LOCK_INIT;
+	}
+	return self;
+}
